{
    "name": "Slickiss",
    "version": "1.7.7",
    "manifest_version": 2,
    "description": "Hides ads and Captcha in Kissanime and offers many other functionalities like notifications for Anime, quick download, and more",
    "browser_action": {
        "default_name": "Slickiss",
        "default_popup": "popup.html",
        "default_icon": "icons/128.png"
    },
    "icons": {
        "128": "icons/128.png"
    },
    "permissions": [
        "*://kissanime.ru/*",
        "*://kissasian.sh/*",
        "*://www.rapidvideo.com/*",
        "*://streamango.com/*",
        "*://ping.idocdn.com/",
        "*://www.mp4upload.com/*",
        "*://www.novelplanet.me/*",
        "*://cutt.ly/*",
        "*://nraisgroupe.com/*",
        "storage",
        "activeTabs",
        "notifications",
        "contextMenus",
        "webRequest",
        "webRequestBlocking",
        "*://vagwyn.pw/*",
        "*://*.ti553.com/*",
        "*://*.popads.net/*",
        "*://*.bebi.com/*",
        "*://1b6a637cbe7bb65ac.com/*",
        "*://4f6b2af479d337cf.com/*",
        "*://4702fb341ddf276d.com/*",
        "*://go.onclasrv.com/*",
        "*://deloton.com/*",
        "*://mc.yandex.ru/*",
        "*://ads.2mdnsys.com/*",
        "*://pl14474491.puserving.com/*",
        "*://go.oclaserver.com/*",
        "*://*.addthis.com/*",
        "*://m.addthisedge.com/*",
        "*://kissanime.ru/ads/*",
        "*://kissanime.ru/AGBXSKCSYWBSDAPOLA",
        "*://kissanime.ru/Scripts/video-js/video.js",
        "*://zukxd6fkxqn.com/*",
        "*://native.propellerads.com/*",
        "*://native.propellerclick.com/*",
        "*://cobalten.com/*",
        "*://pushance.com/*",
        "*://rotumal.com/*",
        "*://pusheify.com/*",
        "*://luckypushh.com/*",
        "*://koindut.com/*",
        "*://wranjeon.xyz/*",
        "*://forcedolphin.com/*",
        "*://nauchegy.link/*",
        "*://bristlyapace.com/*",
        "*://inpagepush.com/*",
        "*://boyaidare.club/*",
        "*://artapeare.site/*",
        "*://ie8eamus.com/*",
        "*://jqk72ugyl2pz.com/*",
        "*://d72gb9oukw3j.com/*",
        "*://983jdh.xyz/*",
        "*://obefjbb4mykw.com/*",
        "*://www.mp4upload.com/vjs/ads.js",
        "*://direct.apus.tech/*"
    ],
    "background": {
        "scripts": [
            "jquery-2.1.4.min.js",
            "md5.js",
            "src/Chrome.js",
            "kickout.js",
            "background.js"
        ],
        "persistent": true
    },
    "web_accessible_resources": [
        "imgs/*",
        "player.html",
        "prompt.html"
    ],
    "content_scripts": [
        {
            "js": [
                "jquery-2.1.4.min.js",
                "ocrad.min.js",
                "md5.js",
                "src/custom.js",
                "src/Tooltip.js",
                "src/LocalStorage.js",
                "src/Prompt.js",
                "src/Assets.js",
                "src/ImgProc.js",
                "src/Chrome.js",
                "src/Notifier.js",
                "src/Captcha.js",
                "src/DlGrabber.js",
                "src/EpisodeListing.js",
                "src/Thumbnail.js",
                "src/Player.js",
                "src/Slickiss.js",
                "src/Bookmark.js",
                "src/Anime.js",
                "src/Episode.js",
                "src/MessageManager.js"
            ],
            "css": [
                "custom-style.css"
            ],
            "matches": [
                "*://kissanime.ru/*",
                "*://kissasian.sh/*",
                "*://playhydrax.com/*",
                "*://hydrax.net/*",
                "*://www.mp4upload.com/*",
                "*://www.novelplanet.me/*"
            ],
            "include_globs": [
                "*://kissanime.ru/*",
                "*://kissasian.sh/*",
                "*://playhydrax.com/*",
                "*://hydrax.net/watch?v=*",
                "*://www.mp4upload.com/embed-*",
                "*://www.novelplanet.me/v/*"
            ],
            "all_frames": true,
            "run_at": "document_start"
        },
        {
            "js": [
                "sendUpdates.js"
            ],
            "matches": [
                "*://kissanime.ru/"
            ],
            "include_globs": [
                "*://kissanime.ru/#sendUpdates"
            ],
            "all_frames": true,
            "run_at": "document_start"
        },
        {
            "js": [
                "src/CaptchaDisplay.js"
            ],
            "matches": [
                "*://kissanime.ru/Special/*"
            ],
            "all_frames": true,
            "run_at": "document_start"
        }
    ]
}